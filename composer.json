{
    "name": "opportify/opportify-sdk-php",
    "description": "Opportify Insights PHP SDK",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Opportify\\Sdk\\": "src/",
            "OpenAPI\\Client\\": "lib/v1/lib"
        }
    },
    "require": {
        "php": ">=8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
