{
    "name": "opportify/opportify-sdk-php",
    "description": "Opportify Insights PHP SDK",
    "type": "library",
    "keywords": [
        "php",
        "opportify",
        "sdk",
        "api",
        "client",
        "email",
        "ip",
        "insights",
        "validation",
        "verification"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Opportify\\Sdk\\": "src/",
            "OpenAPI\\Client\\": "lib/v1/lib"
        }
    },
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}